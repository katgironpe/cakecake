class AppData
  @game_name = "template"
  @width = 360
  @height = 240
  @scale = 2
  @grid_on = false
  @entities:
   'Board': Board
   'GameController': GameController
  @sprites:
   'Ball': 'Ball.png'
   'Heart': 'Heart.png'
   'Pad1': 'Pad1.png'
   'Pad2': 'Pad2.png'
   'PlaceHolder': 'PlaceHolder.png'
  @levels:
   'Level': Level
   'Level2': Level2

